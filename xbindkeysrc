# Audio
"playerctl play-pause"
  XF86AudioPlay

"playerctl next"
  XF86AudioNext

"playerctl previous"
  XF86AudioPrev

"pactl set-sink-volume @DEFAULT_SINK@ +5%"
  XF86AudioRaiseVolume

"pactl set-sink-volume @DEFAULT_SINK@ -5%"
  XF86AudioLowerVolume

"pactl set-sink-mute @DEFAULT_SINK@ toggle"
  XF86AudioMute

# Brightness
"xbacklight -inc 10"
   XF86MonBrightnessUp

"xbacklight -dec 10"
   XF86MonBrightnessDown

# Print
"flameshot gui"
  m:0x0 + c:107

"flameshot screen -c"
  m:0x1 + c:107
